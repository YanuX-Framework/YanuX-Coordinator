{
  "name": "@yanux/coordinator",
  "version": "1.0.0",
  "description": "YanuX Coordinator",
  "repository": "github:YanuX-Framework/YanuX-Coordinator",
  "private": true,
  "author": {
    "name": "Pedro Albuquerque Santos",
    "email": "pedro@albuquerques.net",
    "url": "https://pedro.albuquerques.net"
  },
  "license": "Apache-2.0",
  "main": "dist/lib.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run compile && npm run package",
    "compile": "npx tsc && npx tsc -p tsconfig-test.json",
    "package": "npx webpack",
    "serve": "npx serve ./dist -l 3010",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all --parallel watch:compile watch:package serve",
    "watch:build": "npm-run-all --parallel watch:compile watch:package",
    "watch:compile": "npm-run-all --parallel watch:compile-lib watch:compile-test",
    "watch:compile-lib": "npx tsc --watch",
    "watch:compile-test": "npx tsc -p tsconfig-test.json --watch",
    "watch:package": "npx webpack --watch",
    "start": "npm run serve",
    "test": "node lib/_test/test.js"
  },
  "dependencies": {
    "serve": "^11.3.2"
  },
  "devDependencies": {
    "@feathersjs/authentication-client": "^4.5.9",
    "@feathersjs/errors": "^4.5.9",
    "@feathersjs/feathers": "^4.5.9",
    "@feathersjs/socketio-client": "^4.5.9",
    "@polymer/paper-dialog": "^3.0.1",
    "@types/jsrsasign": "^8.0.6",
    "@types/lodash": "^4.14.161",
    "@types/socket.io-client": "^1.4.34",
    "cross-fetch": "^3.0.6",
    "html-webpack-plugin": "^4.5.0",
    "jsrsasign": "^10.0.0",
    "lit-element": "^2.4.0",
    "lodash": "^4.17.20",
    "node-localstorage": "^2.1.6",
    "node-rules": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^2.3.1",
    "source-map-loader": "^1.1.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
