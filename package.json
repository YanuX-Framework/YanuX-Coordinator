{
  "name": "@yanux/coordinator",
  "version": "1.0.0",
  "description": "YanuX Coordinator",
  "repository": "github:YanuX-Framework/YanuX-Coordinator",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run compile && npm run package",
    "compile": "npx tsc",
    "package": "npx webpack",
    "watch": "npm run watch:build",
    "watch:build": "npm-run-all --parallel watch:compile watch:package",
    "watch:compile": "npx tsc --watch",
    "watch:package": "npx webpack --watch",
    "start": "node lib/index.js",
    "test": "node lib/test.js"
  },
  "author": {
    "name": "Pedro Albuquerque Santos",
    "email": "pedro@albuquerques.net",
    "url": "https://pedro.albuquerques.net"
  },
  "license": "LGPL-3.0",
  "dependencies": {
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio-client": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "node-localstorage": "^1.3.1",
    "node-rules": "^4.0.2",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@types/feathersjs__authentication-client": "^1.0.5",
    "@types/feathersjs__errors": "^3.3.1",
    "@types/feathersjs__feathers": "^3.1.5",
