    "@types/socket.io-client": "^1.4.32",
    "npm-run-all": "^4.1.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
