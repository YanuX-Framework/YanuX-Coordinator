{
  "name": "@yanux/coordinator",
  "version": "1.0.0",
  "description": "YanuX Coordinator",
  "repository": "github:YanuX-Framework/YanuX-Coordinator",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run compile && npm run package",
    "compile": "npx tsc",
    "package": "npx webpack",
    "serve": "npx http-server ./dist -p 3010 -c-1",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all --parallel watch:compile watch:package serve",
    "watch:build": "npm-run-all --parallel watch:compile watch:package",
    "watch:compile": "npx tsc --watch",
    "watch:package": "npx webpack --watch",
    "start": "node lib/index.js",
    "test": "node lib/test.js"
  },
  "author": {
    "name": "Pedro Albuquerque Santos",
    "email": "pedro@albuquerques.net",
    "url": "https://pedro.albuquerques.net"
  },
  "license": "LGPL-3.0",
  "dependencies": {
    "http-server": "^0.12.1"
  },
  "devDependencies": {
    "@feathersjs/authentication": "^4.4.3",
    "@feathersjs/authentication-client": "^4.4.3",
    "@feathersjs/errors": "^4.4.3",
    "@feathersjs/feathers": "^4.4.3",
    "@feathersjs/socketio-client": "^4.4.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jsonwebtoken": "^8.3.6",
    "@types/jsrsasign": "^8.0.2",
    "@types/lodash": "^4.14.149",
    "@types/socket.io-client": "^1.4.32",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsrsasign": "^8.0.12",
    "lit-element": "^2.2.1",
    "lodash": "^4.17.15",
    "node-localstorage": "^2.1.5",
    "node-rules": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^2.3.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
