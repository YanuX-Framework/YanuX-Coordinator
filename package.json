{
  "name": "@yanux/coordinator",
  "version": "1.0.0",
  "description": "YanuX Coordinator",
  "repository": "github:YanuX-Framework/YanuX-Coordinator",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run compile && npm run package",
    "compile": "npx tsc && npx tsc -p tsconfig-test.json",
    "package": "npx webpack",
    "serve": "npx http-server ./dist -p 3010 -c-1",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all --parallel watch:compile watch:package serve",
    "watch:build": "npm-run-all --parallel watch:compile watch:package",
    "watch:compile": "npm-run-all --parallel watch:compile-lib watch:compile-test",
    "watch:compile-lib": "npx tsc --watch",
    "watch:compile-test": "npx tsc -p tsconfig-test.json --watch",
    "watch:package": "npx webpack --watch",
    "start": "npm run serve",
    "test": "node build/test.js"
  },
  "author": {
    "name": "Pedro Albuquerque Santos",
    "email": "pedro@albuquerques.net",
    "url": "https://pedro.albuquerques.net"
  },
  "license": "LGPL-3.0",
  "dependencies": {
    "http-server": "^0.12.1"
  },
  "devDependencies": {
    "@feathersjs/authentication": "^4.5.2",
    "@feathersjs/authentication-client": "^4.5.2",
    "@feathersjs/errors": "^4.5.2",
    "@feathersjs/feathers": "^4.5.2",
    "@feathersjs/socketio-client": "^4.5.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/jsrsasign": "^8.0.2",
    "@types/lodash": "^4.14.149",
    "@types/socket.io-client": "^1.4.32",
    "cross-fetch": "^3.0.4",
    "html-webpack-plugin": "^4.0.4",
    "jsrsasign": "^8.0.13",
    "lit-element": "^2.3.1",
    "lodash": "^4.17.15",
    "node-localstorage": "^2.1.5",
    "node-rules": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^2.3.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
