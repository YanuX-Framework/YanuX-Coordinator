{
  "name": "@yanux/coordinator",
  "version": "1.0.0",
  "description": "YanuX Coordinator",
  "repository": "github:YanuX-Framework/YanuX-Coordinator",
  "private": true,
  "author": {
    "name": "Pedro Albuquerque Santos",
    "email": "pedro@albuquerques.net",
    "url": "https://pedro.albuquerques.net"
  },
  "license": "Apache-2.0",
  "main": "dist/lib.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run compile && npm run package && npm run docs",
    "compile": "npx tsc && npx tsc -p tsconfig-test.json",
    "package": "npx webpack",
    "serve": "npx serve ./dist -l 3010",
    "docs": "npx typedoc src/index.ts",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all --parallel watch:compile watch:package watch:docs serve",
    "watch:build": "npm-run-all --parallel watch:compile watch:package",
    "watch:compile": "npm-run-all --parallel watch:compile-lib watch:compile-test",
    "watch:compile-lib": "npx tsc --watch",
    "watch:compile-test": "npx tsc -p tsconfig-test.json --watch",
    "watch:package": "npx webpack --watch",
    "watch:docs": "npx typedoc src/index.ts --watch",
    "start": "npm run serve",
    "test": "node lib/_test/test.js"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "serve": "^13.0.2"
  },
  "devDependencies": {
    "@feathersjs/authentication-client": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio-client": "^4.5.11",
    "@polymer/paper-dialog": "^3.0.1",
    "@types/jsrsasign": "^8.0.9",
    "@types/lodash": "^4.14.168",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "cross-fetch": "^3.0.6",
    "fast-memoize": "^2.5.2",
    "html-webpack-plugin": "^5.2.0",
    "jose": "^4.3.7",
    "lit-element": "^2.4.0",
    "lodash": "^4.17.21",
    "node-localstorage": "^2.1.6",
    "node-rules": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "socket.io-client": "^2.4.0",
    "source-map-loader": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.2.2",
    "typedoc": "^0.22.4",
    "typescript": "^4.2.2",
    "util": "^0.12.3",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
}
